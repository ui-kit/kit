line-height 1.35em
width 100%
display table

&-row,
&-head,
&-foot
  %./children/row.ess
&-header
  %./children/header.ess
&-cell
  %./children/cell.ess

// I dont know how to get this to work once in the children folder BF
&-row:nth-child(even) > &-cell
  background = __.color.azure
  &:nth-child(even)
    background = __.color.azure.darker



